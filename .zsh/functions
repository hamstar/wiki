function cpuinfo {
  cat /proc/cpuinfo|grep -P 'processor|MHz|model name|cache size'|sed 's/processor/\nprocessor/'
}

function rcheck {
  find . -name \*.rb -exec ruby -c {} \;
}

susvc() {
  sudo service $*;
}

hax() {

  if [ "$1" = "" ]; then 
    cd "$HOME/src";
    return;
  fi;

  cd `find "$HOME/src" -maxdepth 1 -name "*${1}*" -type d|sort|head -1`;
}
